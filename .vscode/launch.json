{
 "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Tests",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "args": [
        "--runInBand",
        "--config",
        "tests/unit-test-setup/jest-config.json"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "console": "internalConsole"
    }
  ]
}