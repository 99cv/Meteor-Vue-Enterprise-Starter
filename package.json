{
  "name": "meteor-vue-modular-app-starter",
  "private": true,
  "scripts": {
    "start": "meteor run --settings config/config.development.json",
    "test:unit": "jest src/ --verbose --coverage --coverageDirectory .coverage/",
    "test:e2e": "chimp --ddp=http://localhost:3000 --mocha --path=tests",
    "lint": "semistandard --parser babel-eslint --env=jest --global server --global browser --verbose | snazzy",
    "lint:fix": "semistandard --parser babel-eslint --env=jest --global server --global browser --verbose  --fix | snazzy",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "meteor-node-stubs": "~0.2.8",
    "vue": "^2.3.3",
    "vue-meteor-tracker": "^1.2.1",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "vuex-alt": "0.0.6",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "chimp": "^0.49.0",
    "chromedriver": "^2.29.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "semistandard": "^11.0.0",
    "snazzy": "^7.0.0"
  }
}
